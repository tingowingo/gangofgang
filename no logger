getgenv().aimhook={}
aimhook.Tabs={}
aimhook.Threads={}

aimhook.GetThreads = function()
    return aimhook.Threads
end
aimhook.CreateLoop = function(name, ltype, callback)
    aimhook.Threads[name]={}
    table.insert(aimhook.Threads[name], ltype)
    table.insert(aimhook.Threads[name], callback)
    local t = true
    table.insert(aimhook.Threads[name], t)
end
aimhook.StartLoop = function(name)
    task.spawn(function()
        if aimhook.Threads[name] and aimhook.Threads[name][1] == 'while' and aimhook.Threads[name][3] == true then
            while task.wait() do
                task.spawn(aimhook.Threads[name]['Callback'][2])    
            end
        end
        
        if aimhook.Threads[name] and aimhook.Threads[name][1] == 'RenderStepped' and aimhook.Threads[name][3] == true then
            game.RunService.RenderStepped:connect(function()
                task.spawn(aimhook.Threads[name][2])
            end)
        end
    end)
end
aimhook.CloseLoop = function(name)
    task.spawn(function()
        aimhook.Threads[name][3] = false
    end)
end
aimhook.Tabs.Aimbot = AimingTab
aimhook.Tabs.Skins = byTab
