    game.Players.LocalPlayer.CharacterAdded:Connect(function(c)
        task.wait(0.25)
        game.Players.LocalPlayer.Character.ChildAdded:Connect(function(x)
            if x:IsA('Tool') then
                if x:FindFirstChild('GunScript_Local') and x:FindFirstChild('Setting') then
                    if configTable.NoRecoil and configTable.Mods.Enabled then
                        hookfunction(getsenv(x.GunScript_Local).RecoilCamera, function()
                            return;    
                        end)
                    end
                    
                    if configTable.Mods.FireRate then
                        local r=require(x.Setting)
                        r.FireRate = 0.02
                    end
                    if configTable.Mods.InfAmmo then
                        local r=require(x.Setting)
                        r.AmmoPerMag = 99999
                    end
                    if configTable.Mods.InstantReload then
                        local r=require(x.Setting)
                        r.ReloadTime=0
                        r.EquipTime=0
                    end
                end
            end
        end)
    end)
    
