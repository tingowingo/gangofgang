local gameData={
    GetCurrentAmmo=function()
        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild('GunGUI') then
            
        end
        
        return 0
    end,
    GetMenuState=function()
        if game.Players.LocalPlayer.PlayerGui.MainMenu.Enabled then
            return true;    
        end
        
        return false
    end,
    GetSecureFolder=function()
        return (workspace and workspace.aimhook or (secureFolder) )
    end,
    GetAlive=function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild('Humanoid') and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
            return (game.Players.LocalPlayer.Character and true or false)
        end
        
        return false;
    end,
    RemoveScope=function()
        if configTable.RemoveScope then
            if game.Players.LocalPlayer.PlayerGui:FindFirstChild('GunGUI') and game:GetService("Players").LocalPlayer.PlayerGui.GunGUI:FindFirstChild('Scope') and game:GetService("Players").LocalPlayer.PlayerGui.GunGUI.Scope:FindFirstChild('top')  then
                game:GetService("Players").LocalPlayer.PlayerGui.GunGUI.Scope.Image = ''
                game:GetService("Players").LocalPlayer.PlayerGui.GunGUI.Scope.top:Destroy()
                game:GetService("Players").LocalPlayer.PlayerGui.GunGUI.Scope.left:Destroy()
                game:GetService("Players").LocalPlayer.PlayerGui.GunGUI.Scope.bottom:Destroy()
                game:GetService("Players").LocalPlayer.PlayerGui.GunGUI.Scope.right:Destroy()
            end
        end
    end,
    GetCTArms=function()
        if workspace.CurrentCamera:FindFirstChild('Arms') then
            return true;    
        end
        
        return false
    end,
    ClearKillers=function()
        if workspace:FindFirstChild'Map' then
            local map = workspace.Map:FindFirstChildOfClass('Model') 
            
            if (map) then
                local map = workspace.Map:FindFirstChildOfClass('Model')


                for _, v in next, map:GetChildren()do
                	if v:IsA('BasePart') and v.Color == Color3.fromRGB(255, 0, 0) then
                		v:Destroy()
                	end
                end
            end
        end
    end,
    -- returns the spread func
    GetSpread=nil
}
