function getAccurateSleeve()
    
        if configTable.SkinChanger.SleeveTexture == 'Amethyst' then
            return 'rbxassetid://685632644'    
        end
        if configTable.SkinChanger.SleeveTexture == 'Anime 1' then
            return 'http://www.roblox.com/asset/?id=434150984'    
        end
        if configTable.SkinChanger.SleeveTexture == "Black 'n White" then
            return 'http://www.roblox.com/asset/?id=8126977748'    
        end
        
        if configTable.SkinChanger.SleeveTexture == 'Galaxy' then
            return 'rbxassetid://11928277386'    
        end
        
        if configTable.SkinChanger.SleeveTexture == 'Drip' then
            return 'rbxassetid://11928068161'    
        end
        if configTable.SkinChanger.SleeveTexture == 'Fire' then
            return 'rbxassetid://34333681'    
        end
        if configTable.SkinChanger.SleeveTexture == 'Pastel' then
            return 'rbxassetid://11928134400'    
        end 
end
skinSec2:AddButton('Unlock All Sleeves',function(x)
        while task.wait() do
            if #game:GetService("Players").LocalPlayer.PlayerGui.MainMenu.Frame.SleeveInventory.PrimarySkins.ScrollingFrame:GetChildren() < 2 then
                for i,v in pairs(game:GetService("ReplicatedStorage").ViewmodelSkins:GetChildren()) do
                    local Template = game:GetService("Players").LocalPlayer.PlayerGui.MainMenu.Frame.SleeveInventory.VMInventoryHandler.Item:Clone()
                    Template.Name = v.Name
                        Template.Parent = game:GetService("Players").LocalPlayer.PlayerGui.MainMenu.Frame.SleeveInventory.PrimarySkins.ScrollingFrame
                    Template.ItemName.Text = v.DisplayName.Value
                    Template.Image = v.SkinID.Value
                    Template.MouseButton1Click:Connect(function()
                        game:GetService("ReplicatedStorage").Events.EquipVM:FireServer(true, v.SkinID.Value)
                    end)
                end
        end
    end    
end)
skinSec2:AddButton('Update Skins', function(x)
    if configTable.SkinChanger.UseCustomSleeve then
        local args = {
            [1] = true,
            [2] = configTable.SkinChanger.CustomSleeveTexture
        }
    
        game:GetService("ReplicatedStorage").Events.EquipVM:FireServer(unpack(args))
        return
    end
    
    local args = {
        [1] = true,
        [2] = getAccurateSleeve()
    }
    
    game:GetService("ReplicatedStorage").Events.EquipVM:FireServer(unpack(args))
end)
