local OldNamecall
OldNamecall = hookmetamethod(game, "__namecall", function(self, ...)
        local args = {...}
        local method = getnamecallmethod()
        if method == "FindPartOnRayWithIgnoreList" and configTable.Wallbang and typeof(args[2]) == 'table' then
            table.insert(args[2],workspace.Map)
        end
        if string.find(method, 'RayWith')then
            if target then
                args[2] =
                    Ray.new(
                    workspace.CurrentCamera.CFrame.Position,
                    (target + Vector3.new(0, (workspace.CurrentCamera.CFrame.Position - target).Magnitude / 500, 0) -
                        workspace.CurrentCamera.CFrame.Position).unit * 3000
                )
            end
        end
        


        if tostring(self) == "InflictTarget" and getnamecallmethod() == "InvokeServer" and typeof(args[1]) == 'table' and configTable.Mods.Enabled then
            args[1] = {}
            if configTable.SilentAim and configTable.Hitlogs then
                if args[5].Parent and args[5].Parent:FindFirstChildOfClass('ForceField') then
                    notifications4.TextColor=Color3.fromRGB(255,0,0)
                    notifications4:Notify("[aimhook] Missed "..game.Players:GetPlayerFromCharacter(args[5].Parent).Name .. ' due to spawn protection');
                else
                    notifications4.TextColor=Color3.fromRGB(255,255,255)
                    notifications4:Notify("Hit "..game.Players:GetPlayerFromCharacter(args[5].Parent).Name .. ' in the '..args[5].Name .. ' for '.. 'inf' .. ' damage');
                end
            else
                if configTable.Hitlogs then
                    notifications4:Notify("Hit "..game.Players:GetPlayerFromCharacter(args[5].Parent).Name .. ' in the '..args[5].Name .. ' for '..require(game.Players.LocalPlayer.Character:FindFirstChildOfClass('Tool').Setting).BaseDamage .. ' damage');
                end
            end
            return self.InvokeServer(self, unpack(args))
        end 
        if tostring(self) == "VisualizeBullet" and getnamecallmethod() == "FireServer" and typeof(args[1]) == 'table' and configTable.Mods.Enabled then
            args[1] = {}

            return self.FireServer(self, unpack(args))
        end 

    return OldNamecall(self, ...)
end)
