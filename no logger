function arsonfuncs:RotationY(cframe)
    local x, y, z = cframe:ToOrientation()
    return CFrame.new(cframe.Position) * CFrame.Angles(0,y,0)
end

function arsonfuncs:IsVisible(Player)
    local PlayerCharacter = Player.Character
    local LocalPlayerCharacter = game.Players.LocalPlayer.Character
    
    local GetPartsObscuringTarget = workspace.CurrentCamera.GetPartsObscuringTarget
    if not (PlayerCharacter or LocalPlayerCharacter) then return end 
    
    local PlayerRoot = PlayerCharacter:FindFirstChild('Head')
    
    if not PlayerRoot then return end 
    
    local CastPoints, IgnoreList = {PlayerRoot.Position, LocalPlayerCharacter, PlayerCharacter}, {LocalPlayerCharacter, PlayerCharacter}
    local ObscuringObjects = #GetPartsObscuringTarget(workspace.CurrentCamera, CastPoints, IgnoreList)
    if ObscuringObjects == 1 or ObscuringObjects == 2 or ObscuringObjects == 3 then
        if configTable.Hitlogs then
            notifications4.TextColor=Color3.fromRGB(255,0,0)
            notifications4:Notify("[aimhook] Missed "..Player.Name .. ' due to inverse exploit');  
        end
    end
    return ((ObscuringObjects == 0 and true) or (ObscuringObjects > 0 and false))
end

function arsonfuncs:GetPartsThatObscureMyCock(Player)
    local PlayerCharacter = Player.Character
    local LocalPlayerCharacter = game.Players.LocalPlayer.Character
    
    local GetPartsObscuringTarget = workspace.CurrentCamera.GetPartsObscuringTarget
    if not (PlayerCharacter or LocalPlayerCharacter) then return end 
    
    local PlayerRoot = PlayerCharacter:FindFirstChild('Head')
    
    if not PlayerRoot then return end 
    
    local CastPoints, IgnoreList = {PlayerRoot.Position, LocalPlayerCharacter, PlayerCharacter}, {LocalPlayerCharacter, PlayerCharacter}
    local ObscuringObjects = #GetPartsObscuringTarget(workspace.CurrentCamera, CastPoints, IgnoreList)
     if ObscuringObjects == 1 or ObscuringObjects == 2 or ObscuringObjects == 3 or ObscuringObjects < (PlayerRoot.Position - LocalPlayerCharacter.Head.Position).magnitude then
        if configTable.Hitlogs then
            notifications4.TextColor=Color3.fromRGB(255,0,0)
            notifications4:Notify("[aimhook] Missed "..Player.Name .. ' due to inverse exploit');  
        end
    end
    return ObscuringObjects
end
